{
    "manifest_version": 2,

    "name": "MOABB",
    "description": "This extension allows the user to use the back button through redirect pages",
    "version": "1.0",

    "permissions": [
        "webNavigation",
        "browsingData",
        "tabs"
    ],

    "background": {
        "scripts": ["background.js"]
    },

    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["jquery-3.3.1.min.js", "content.js"]
        }
    ]
}
